{
  "name": "@rune/cli",
  "description": "The CLI for Rune, a flexible and expressive code generator",
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "author": {
    "name": "Forgebyd",
    "email": "ini.dwiii@gmail.com",
    "url": "https://github.com/forgebyd"
  },
  "homepage": "https://github.com/forgebyd/rune",
  "readme": "https://github.com/forgebyd/rune/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forgebyd/rune.git"
  },
  "volta": {
    "node": "24.11.0",
    "pnpm": "10.20.0"
  },
  "bin": {
    "rune": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "dev": "./source/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">20.0.0 || >=24.11.0"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "keywords": [
    "forgebyd",
    "rune",
    "cli",
    "command",
    "command-line",
    "generator",
    "file-generator",
    "code-generator",
    "automation",
    "initialization",
    "productivity",
    "scaffolding"
  ],
  "scripts": {
    "build": "tsdown",
    "start": "node dist/cli.mjs",
    "start.dev": "node --conditions dev dist/cli.mjs",
    "dev": "tsx --conditions dev bin/cli.ts",
    "package.lint": "publint"
  },
  "dependencies": {
    "@gunshi/bone": "^0.26.3",
    "@gunshi/definition": "^0.26.3",
    "@gunshi/plugin": "^0.26.3",
    "@gunshi/plugin-global": "^0.26.3",
    "@gunshi/plugin-renderer": "^0.26.3"
  }
}
